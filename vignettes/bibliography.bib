@MastersThesis{Valade2010,
  author      = {Valade, D.},
  title       = {Proposition d'une méthode d'échantillonnage du phytobenthos en plan d'eau},
  year        = {2010},
  type        = {mathesis},
  note        = {Mémoire d'élève},
  abstract    = {Depuis des dizaines d'années un travail considérable a été effectué sur le phytobenthos en rivière, ce travail a abouti à l'harmonisation des méthodes d'échantillonnage et à l'élaboration d'indices diatomiques permettant l'évaluation de la qualité des cours d'eau. La Directive Cadre européenne sur l'Eau impose aujourd'hui aux pays membres d'utiliser le phytobenthos pour qualifier l'état écologique des plans d'eau. Mon travail bibliographique m'a permis de recenser les différentes méthodes d'échantillonnage existantes. Ce rapport fait ainsi référence à celles-ci mais s'attarde principalement sur l'échantillonnage du phytobenthos sur hélophytes. En m'appuyant sur les différentes méthodes européennes actuelles et l'avis d'experts, j'ai réalisé un protocole traitant des phases d'échantillonnage jusqu'à la production d'indices diatomiques appropriés aux milieux lacustres français. Ce protocole répond aux attentes de la DCE et sera testé en 2011 par l'agence de l'eau Rhône-Méditerranée-Corse lors de campagnes pilotes sur les lacs de références.},
  affiliation = {CEMAGREF BORDEAUX UR REBX FRA},
  keywords    = {DIATOMEE ; LAC ; PHYTOBENTHOS ; DIRECTIVE CADRE EUROPEENNE SUR L'EAU},
  pages       = {17},
  publisher   = {Master 1 Sciences, Terre et Environnement},
  url         = {http://irsteadoc.irstea.fr/cemoa/PUB00029181},
}

@techreport{PUB00030275,
   note = {Rapport scientifique},
   author = {Morin, S. and Valade, D. and Tison Rosebery, J. and Bertrin, V. and Cellamare, M. and Dutartre, A.},
   title = {Utilisation du phytobenthos pour la bioindication en plans d'eau : Etat de l'art des méthodes disponibles et test de métriques sur les plans d'eau aquitains},
   pages = {56},
   url = {http://irsteadoc.irstea.fr/cemoa/PUB00030275},
   year = {2010},
   affiliation = {CEMAGREF BORDEAUX UR REBX FRA ; CEMAGREF BORDEAUX UR REBX FRA ; CEMAGREF BORDEAUX UR REBX FRA ; CEMAGREF BORDEAUX UR REBX FRA ; CEMAGREF BORDEAUX UR REBX FRA ; CEMAGREF BORDEAUX UR REBX FRA},
   abstract = {Les différents éléments de qualité biologique (EQB) concernés par la mise en oeuvre de la Directive Cadre européenne sur l'Eau (DCE 2000/60/CE) et utilisés comme indicateurs écologiques pour l'évaluation des eaux de surface sont le phytoplancton, le phytobenthos, les macrophytes, les invertébrés benthiques et les poissons. Le présent travail compare les différentes méthodes existantes pour l'évaluation de l'état écologique des plans d'eau à partir des communautés phytobenthiques et propose une application de celles-ci dans cinq lacs naturels situés dans la région Aquitaine. Plusieurs indices biologiques utilisés par d'autres pays européens en lacs ou en cours d'eau ont été appliqués et comparés, démontrant une sensibilité adéquate de la plupart des indices. Parmi eux, l'indice français IBD2007 donne une vision globale assez juste de l'état écologique de ces plans d'eau, et, moyennant des améliorations, pourrait être utilisé comme métrique nationale d'évaluation de la qualité des milieux lacustres. Ces améliorations à apporter à l'indice concerneraient principalement la réévaluation de certains profils écologiques d'espèces rarement rencontrées en cours d'eau, l'intégration de nouvelles espèces typiques des plans d'eau et l'intégration d'éléments concernant le caractère invasif d'espèces exotiques. Ce rapport propose également un protocole d'échantillonnage DCE-compatible pour la mise en oeuvre standardisée des prélèvements de phytobenthos en plans d'eau, préconisant un échantillonnage sur macrophytes émergées  sur des sections de tige localisées en zone photique et en contact permanent avec la masse d'eau afin d'estimer au mieux la qualité de l'eau et non celle de l'herbier. Quelques éléments relatifs aux premières étapes d'intercalibration européenne des méthodes basées sur le phytobenthos pour l'évaluation de la qualité des lacs sont également fournis ici. Des analyses préliminaires utilisant les données acquises sur les lacs aquitains, à partir desquelles a été calculé l'IBD2007, indiquent que cette métrique pourrait être utilisée dans le cadre des exercices d'intercalibration européenne.},
   keywords = {DIATOMEE ; INDICE BIOLOGIQUE DIATOMEE ; ECHANTILLONNAGE ; PLAN D'EAU ; BIOINDICATION ; PHYTOBENTHOS ; ETALONNAGE ; DIRECTIVE CADRE EUROPEENNE SUR L'EAU},
}

